    "v-wave": "^3.0.4",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@localazy/cli": "^2.0.8",
    "@stylistic/stylelint-plugin": "^5.0.1",
    "@tabler/icons-vue": "^3.36.1",
