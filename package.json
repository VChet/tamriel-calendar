  "dependencies": {
    "@unhead/vue": "^2.0.19",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.9.0",
    "dayjs": "^1.11.18",
    "v-wave": "^3.0.4",
    "vite": "^7.1.10",
    "vite-plugin-pwa": "^1.1.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@localazy/cli": "^2.0.8",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@tabler/icons-vue": "^3.35.0",
    "@total-typescript/ts-reset": "^0.6.1",
